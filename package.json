{
  "name": "my-spotify-stats",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod=true && ./scripts/copy-files.sh",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix=true",
